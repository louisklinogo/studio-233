# Database
# For Neon, add ?connect_timeout=30&pool_timeout=30 to the connection string
DATABASE_URL="postgresql://user:password@host:port/db?sslmode=require&connect_timeout=30&pool_timeout=30"
DIRECT_DATABASE_URL="postgresql://user:password@host:port/db?sslmode=require&connect_timeout=30"

# LLM Provider API Keys
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# LangWatch
LANGWATCH_API_KEY=your_langwatch_api_key_here
