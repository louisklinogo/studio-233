{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"globalPassThroughEnv": [
		"DATABASE_URL",
		"DIRECT_DATABASE_URL",
		"BLOB_READ_WRITE_TOKEN",
		"GOOGLE_GENERATIVE_AI_API_KEY",
		"FAL_KEY",
		"KV_REST_API_TOKEN",
		"KV_REST_API_URL",
		"GOOGLE_CLIENT_ID",
		"GOOGLE_CLIENT_SECRET",
		"BETTER_AUTH_SECRET",
		"CORS_ORIGIN",
		"BETTER_AUTH_URL",
		"RESEND_API_KEY",
		"RESEND_FROM_EMAIL",
		"PLAYWRIGHT_BROWSERS_PATH"
	],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"format:check": {
			"dependsOn": ["^format:check"]
		},
		"format:write": {
			"dependsOn": ["^format:write"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"db:push": {
			"cache": false,
			"persistent": true
		},
		"db:studio": {
			"cache": false,
			"persistent": true
		},
		"db:migrate": {
			"cache": false,
			"persistent": true
		},
		"db:generate": {
			"cache": false,
			"persistent": true
		},
		"start": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		},
		"inngest:dev": {
			"cache": false,
			"persistent": true
		}
	}
}
